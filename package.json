{
  "name": "docs.threatx.com",
    "devDependencies": {
        "@antora/cli": "3.1.9",
        "@antora/site-generator": "3.1.9",
        "yaml-lint": "^1.2.4"
    },
    "dependencies": {
        "@antora/lunr-extension": "^1.0.0-alpha.8",
        "@antora/pdf-extension": "^1.0.0-alpha.9",
        "@docsearch/js": "^3.6.1",
        "@fontsource/montserrat": "^5.0.20",
        "asciidoctor-emoji": "^0.5.0",
        "asciidoctor-opal-runtime": "^2.1.0",
        "highlight.js": "^11.10.0",
        "node-html-parser": "^6.1.13"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "check": "npx yamllint -c .yamllint.yml antora-playbook.yml",
        "build": "npx antora --fetch --stacktrace antora-playbook.yml",
        "build-author": "npx antora --fetch --stacktrace antora-playbook-author.yml",
        "serve": "npx http-server ./public/",
        "bundle": "bundle config set path .bundle/gems && bundle",
        "ruby-install": "rvm install 3.3.5 && rvm use 3.3.5",
        "clean": "rm -rf ./build ./public ./.bundle ./node_modules ./.cache"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ThreatX/docs.threatx.com.git"
    },
    "keywords": [
        "Antora",
        "AsciiDoc",
        "tooling"
    ],
    "author": "Nick Ferguson <nick.ferguson@threatx.com>_",
    "homepage": "https://gitub.com/ThreatX/docs.threatx.com"
}
